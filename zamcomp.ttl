@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#>.
@prefix ue: <http://lv2plug.in/ns/extensions/units#>.
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#>.

<http://zamaudio.com/lv2/zamcomp#ui_gtk>
	a ui:GtkUI;
	ui:binary <zamcompUI_gtk.so>.

<http://zamaudio.com/lv2/zamcomp>
	a lv2:Plugin;
	a lv2:CompressorPlugin;

	lv2:binary <zamcomp.so>;
	ui:ui <http://zamaudio.com/lv2/zamcomp#ui_gtk>;
	doap:name "ZamComp";
	doap:maintainer [
		foaf:name "Damien Zammit";
		foaf:homepage <http://zamaudio.com>;
		foaf:mbox <mailto:damien@zamaudio.com>;
		];
	doap:license <http://usefulinc.com/doap/licenses/gpl>;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:minorVersion 2 ;
	lv2:microVersion 1 ;
  
lv2:port [
	a lv2:AudioPort, lv2:InputPort;
	lv2:index 0;
	lv2:symbol "input";
	lv2:name "Input";
	],

	[
	a lv2:AudioPort, lv2:OutputPort;
	lv2:index 1;
	lv2:symbol "output";
	lv2:name "Output";
	],

	[
	a lv2:ControlPort, lv2:InputPort ;
	lv2:index 2;
	lv2:symbol "attack";
	lv2:name "Attack (ms)";
	lv2:minimum 0.25 ;
	lv2:maximum 80.0 ;
	lv2:default 10.0 ;
	ue:unit ue:ms;
	],

	[
	a lv2:ControlPort, lv2:InputPort ;
	lv2:index 3;
	lv2:symbol "release";
	lv2:name "Release (ms)";
	lv2:minimum 2.0 ;
	lv2:maximum 500.0 ;
	lv2:default 80.0 ;
	ue:unit ue:ms;
	],

	[
	a lv2:ControlPort, lv2:InputPort ;
	lv2:index 4;
	lv2:symbol "knee";
	lv2:name "Knee (dB)";
	lv2:minimum 0.0 ;
	lv2:maximum 30.0 ;
	lv2:default 3.0 ;
	ue:unit ue:db;
	],
	
	[
	a lv2:ControlPort, lv2:InputPort ;
	lv2:index 5;
	lv2:symbol "ratio";
	lv2:name "Ratio (dB)";
	lv2:minimum 1.0 ;
	lv2:maximum 20.0 ;
	lv2:default 3.0 ;
	ue:unit ue:db;
	],
	
	[
	a lv2:ControlPort, lv2:InputPort ;
	lv2:index 6;
	lv2:symbol "threshold";
	lv2:name "Threshold (dB)";
	lv2:minimum -60.0 ;
	lv2:maximum 0.0 ;
	lv2:default 0.0 ;
	ue:unit ue:db;
	],
	
	[
	a lv2:ControlPort, lv2:InputPort ;
	lv2:index 7;
	lv2:symbol "makeup";
	lv2:name "Makeup Gain (dB)";
	lv2:minimum 0.0 ;
	lv2:maximum 30.0 ;
	lv2:default 0.0 ;
	ue:unit ue:db;
	],

	[
	a lv2:ControlPort, lv2:OutputPort;
	lv2:index 8;
	lv2:symbol "gain_reduction";
	lv2:name "Gain Reduction (dB)";
	lv2:minimum 0.0 ;
	lv2:maximum 12.0 ;
	lv2:default 0.0 ;
	lv2:scalePoint [ rdfs:label "0"; rdf:value 0 ] ;
	lv2:scalePoint [ rdfs:label "6"; rdf:value 6 ] ;
	lv2:scalePoint [ rdfs:label "12"; rdf:value 12 ] ;
	].
